#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a = 3.0;
    double b = 2.712;

    double numerator = 0.807 * (1 - pow(sin((a - b) * M_PI / 4.0), 2));

    double denominator = 0.312 * (1 + pow(cos((a + b) * M_PI / 4.0), 2));

    double exponent = numerator / denominator;

    double base = pow((pow(a, 2) / pow(b, 3)), 2.0 / 3.0);

    double f = base * exp(exponent);

    cout << "f = " << f << endl;

    return 0;
}
