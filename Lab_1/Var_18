#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a = 1.0;
    double b = 0.001;

    double insideCos = cos((a - b) * M_PI / 8.0);

    double numerator = log(insideCos);

    double denominator = 0.137;

    double fraction = numerator / denominator;

    double cubeRoot = cbrt(((a - b) / (a + b)) * exp(1.0));

    double insideSqrt = fraction * cubeRoot;

    double f = sqrt(insideSqrt);

    cout << "f = " << f << endl;

    return 0;
}
