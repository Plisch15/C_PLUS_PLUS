#include <iostream>
#include <cmath>
using namespace std;

int main() {
  double a = 3;
  double a = 2,712;
    
    const double PI = acos(-1.0);
    double numerator   = 0.807 * (1 - pow(sin((a - b) * PI / 4.0), 2));
    double denominator = 0.312 * (1 + pow(cos((a + b) * PI / 4.0), 2));

    double fraction = numerator / denominator;
    double powerPart = pow((a * a) / pow(b, 3), 2.0 / 3.0);
    
    double result = powerPart * exp(1.0) * fraction;

    cout << "Результат = " << result << endl;

    return 0;
}
