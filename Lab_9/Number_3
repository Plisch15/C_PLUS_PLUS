#include <iostream>
#include <vector>
#include <algorithm>

using namespace std; 

int main() {
    int n;
    cin >> n;       
    if (n <= 1) return 1;

    vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        cin >> a[i];    
    }

    int minIndex = 0;
    int maxIndex = 0;
    for (int i = 1; i < n; ++i) {
        if (a[i] < a[minIndex]) minIndex = i;
        if (a[i] > a[maxIndex]) maxIndex = i;
    }

    swap(a[minIndex], a[maxIndex]);

    for (int i = 0; i < n; ++i) {
        if (i) cout << ' ';   
        cout << a[i];
    }
    cout << '\n';

    return 0;
}
