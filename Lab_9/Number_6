#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cin >> n;
    if (n <= 0) return 1;

    vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        cin >> a[i];
    }

    int index = -1;
    for (int i = 0; i < n; ++i) {
        if (a[i] == 0) {
            index = i;
            break;
        }
    }

    if (index != -1)
        cout << index << '\n';
    else
        cout << "No zero elements\n";

    return 0;
}
