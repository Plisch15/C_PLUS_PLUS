#include <iostream>
#include <vector>

int main() {
    int n;
    std::cin >> n;
    if (n <= 0) return 1;

    std::vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> a[i];
    }

    int index = -1;
    for (int i = 0; i < n; ++i) {
        if (a[i] == 0) {
            index = i;
            break;
        }
    }

    if (index != -1)
        std::cout << index << '\n';
    else
        std::cout << "No zero elements\n";

    return 0;
}
