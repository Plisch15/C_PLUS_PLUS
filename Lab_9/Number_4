#include <iostream>
#include <vector>

int main() {
    int n;
    std::cin >> n;
    if (n <= 0) return 1;

    std::vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        std::cin >> a[i];
    }

    int A;
    std::cin >> A;

    int count = 0;
    for (int i = 0; i < n; ++i) {
        if (a[i] > A) count++;
    }

    std::cout << count << '\n';
    return 0;
}
