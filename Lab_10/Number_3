#include <iostream>

int main() {
    const int m = 5;
    const int n = 7;
    int a[m][n];

    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            std::cin >> a[i][j];
        }
    }

    for (int i = 0; i < m; ++i) {
        int rowSum = 0;
        for (int j = 0; j < n; ++j) {
            rowSum += a[i][j];
        }
        std::cout << "Row " << i << ": " << rowSum << "\n";
    }

    return 0;
}
