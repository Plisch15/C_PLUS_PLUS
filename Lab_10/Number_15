#include <iostream>

using namespace std;

int main() {
    const int m = 3;
    const int n = 3;
    int a[m][n];

    for (int i = 0; i < m; ++i) {
        for (int j = 0; j < n; ++j) {
            cin >> a[i][j];
        }
    }

    int mainDiagonalSum = 0;
    int secondaryDiagonalSum = 0;

    for (int i = 0; i < m; ++i) {
        mainDiagonalSum += a[i][i];
        secondaryDiagonalSum += a[i][n - 1 - i];
    }

    cout << "Main diagonal sum: " << mainDiagonalSum << "\n";
    cout << "Secondary diagonal sum: " << secondaryDiagonalSum << "\n";

    return 0;
}
