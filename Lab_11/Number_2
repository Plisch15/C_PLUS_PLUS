#include <iostream>

using namespace std;

struct Time {
    int hours = 0;
    int minutes = 0;
};

struct Date {
    int day = 1;
    int month = 1;
    int year = 0;
};

struct DateTime {
    Date date;
    Time time;
};

int totalMinutes(const DateTime& dt) {
    int daysSinceStart = dt.date.year * 365 + dt.date.month * 30 + dt.date.day;
    int minutes = daysSinceStart * 24 * 60 + dt.time.hours * 60 + dt.time.minutes;
    return minutes;
}

int main() {
    DateTime start, end;

    cout << "Enter start date (day month year): ";
    cin >> start.date.day >> start.date.month >> start.date.year;
    cout << "Enter start time (hours minutes): ";
    cin >> start.time.hours >> start.time.minutes;

    cout << "Enter end date (day month year): ";
    cin >> end.date.day >> end.date.month >> end.date.year;
    cout << "Enter end time (hours minutes): ";
    cin >> end.time.hours >> end.time.minutes;

    if (start.time.hours < 0 || start.time.hours >= 24 || start.time.minutes < 0 || start.time.minutes >= 60 ||
        end.time.hours < 0 || end.time.hours >= 24 || end.time.minutes < 0 || end.time.minutes >= 60) {
        cout << "Invalid input\n";
        return 1;
    }

    int startTotal = totalMinutes(start);
    int endTotal = totalMinutes(end);

    if (endTotal < startTotal) {
        cout << "End date-time must be after start date-time\n";
        return 1;
    }

    int durationMinutes = endTotal - startTotal;

    Time duration;
    duration.hours = durationMinutes / 60;
    duration.minutes = durationMinutes % 60;

    cout << "Duration: " << duration.hours << ":" << duration.minutes << "\n";

    return 0;
}
