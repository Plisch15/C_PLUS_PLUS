#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main() {
    double a, b, h, x, y;

    cout << "Enter a: ";
    cin >> a;
    cout << "Enter b: ";
    cin >> b;
    cout << "Enter step h: ";
    cin >> h;

    cout << "\n--------------------------------\n";
    cout << "|     x     |       y       |\n";
    cout << "--------------------------------\n";

    x = a;
    while (x <= b + 1e-9) {
        if (x >= 0) {
            y = pow(x, 0.25) + 6;
            cout << "| " << setw(9) << fixed << setprecision(4) << x 
                 << " | " << setw(13) << fixed << setprecision(6) << y << " |\n";
        } else {
            cout << "| " << setw(9) << fixed << setprecision(4) << x 
                 << " |   not in domain  |\n";
            break;
        }
        x += h;
    }
    cout << "--------------------------------\n";

    return 0;
}
