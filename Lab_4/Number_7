#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    float a, b, c, d, e;

    cin >> a >> b >> c >> d >> e;

    cout << a << endl;                       // default format
    cout << fixed << setprecision(2) << b << endl; // 2 decimal places
    cout << fixed << setprecision(6) << c << endl; // 6 decimal places
    cout << fixed << setprecision(2) << d << endl; // 2 decimal places
    cout << static_cast<int>(e) << endl;     // integer part only

    return 0;
}
